<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="supplierinfo_group_view_form" model="ir.ui.view">
        <field name="model">product.supplierinfo.group</field>
        <field name="inherit_id" ref="product_supplierinfo_group.supplierinfo_group_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='supplierinfo_ids']//field[@name='price']" position="after">
                <field name="sale_margin"/>
            </xpath>
        </field>
    </record>
</odoo>
