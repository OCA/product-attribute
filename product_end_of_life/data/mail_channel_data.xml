<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
        <record id="channel_product_eol" model="mail.channel">
            <field name="name">Product End-Of-Life Channel</field>
            <field name="description">Product End-Of-Life Group Notification.</field>
            <field name="email_send">True</field>
            <field name="public">private</field>
            <field name="channel_last_seen_partner_ids" eval="[(4, ref('base.user_root'))]"/>
        </record>        
        <record id="mail_message_product_eol" model="mail.message">
            <field name="model">mail.channel</field>
            <field name="res_id" ref="product_end_of_life.channel_product_eol"/>
            <field name="message_type">comment</field>
            <field name="subject">Product End-Of-Life Notification</field>
            <field name="subtype_id" ref="mail.mt_comment"/>
        </record>
</odoo>
