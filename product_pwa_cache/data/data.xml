<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="0">
        <record id="pwa_cache_product_product_no_big_images" model="pwa.cache">
            <field name="name">Products (All - No Extra Images)</field>
            <field name="cache_type">model</field>
            <field name="model_id" ref="product.model_product_product" />
            <field name="model_domain_raw">[]</field>
            <field
                name="model_field_included_ids"
                eval="[(6, 0, [ref('product.field_product_product__image_128'), ref('product.field_product_product__image_1024'), ref('product.field_product_product__sale_ok'), ref('product.field_product_product__uom_id')])]"
            />
        </record>
        <record id="pwa_cache_product_template_all" model="pwa.cache">
            <field name="name">Product Templates</field>
            <field name="cache_type">model</field>
            <field name="model_id" ref="product.model_product_template" />
            <field name="model_domain_raw">[]</field>
        </record>
        <record
            id="pwa_cache_model_product_template_attribute_value_all"
            model="pwa.cache"
        >
            <field name="name">Product Template Attribute Value (All)</field>
            <field name="cache_type">model</field>
            <field name="model_domain_raw">[]</field>
            <field
                name="model_id"
                ref="product.model_product_template_attribute_value"
            />
        </record>
        <record
            id="pwa_cache_model_product_template_attribute_line_all"
            model="pwa.cache"
        >
            <field name="name">Product Template Attribute Line (All)</field>
            <field name="cache_type">model</field>
            <field name="model_domain_raw">[]</field>
            <field
                name="model_id"
                ref="product.model_product_template_attribute_line"
            />
        </record>
        <record id="pwa_cache_model_product_attribute_all" model="pwa.cache">
            <field name="name">Product Attribute (All)</field>
            <field name="cache_type">model</field>
            <field name="model_domain_raw">[]</field>
            <field name="model_id" ref="product.model_product_attribute" />
        </record>
        <record id="pwa_cache_model_product_attribute_value_all" model="pwa.cache">
            <field name="name">Product Attribute Value (All)</field>
            <field name="cache_type">model</field>
            <field name="model_id" ref="product.model_product_attribute_value" />
            <field name="model_domain_raw">[]</field>
        </record>
        <record id="pwa_cache_model_product_pricelist_all" model="pwa.cache">
            <field name="name">Product Pricelists (All)</field>
            <field name="cache_type">model</field>
            <field name="model_id" ref="product.model_product_pricelist" />
            <field name="model_domain_raw">[]</field>
        </record>
    </data>
</odoo>
